{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "source": {
    "include": [
      "build",
      "package.json",
      "README.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true,
    "useCollapsibles": true,
    "name": "Shortcuts JS",
    "footerText": "Documentation theme by Toast UI",
    "logo": {
      "url": "https://i.imgur.com/Gp4l0Bv.png",
      "width": "200px",
      "height": "50px",
      "link": "https://shortcuts.fun"
    },
    "default": {
      "staticFiles": {
        "include": ["assets/"]
      }
    },
    "css": [
      "docs.css"
    ]
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/tui-jsdoc-template"
  }
}