{
  "name": "@joshfarrant/shortcuts-js",
  "version": "0.3.0",
  "description": "An iOS 12 Shortcuts creator",
  "engineStrict": true,
  "engines": {
    "node": ">= 10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshfarrant/shortcuts-js.git"
  },
  "keywords": [
    "iOS",
    "iOS 12",
    "Shortcuts",
    "Apple",
    "Apple Shortcuts"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "~23.3.9",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.12.8",
    "@types/uuid": "^3.4.4",
    "ajv": "^6.6.1",
    "babel-loader": "^8.0.4",
    "command-line-args": "^5.0.2",
    "coveralls": "^3.0.2",
    "css-loader": "^2.0.1",
    "file-loader": "^3.0.1",
    "husky": "^1.1.4",
    "inquirer": "^6.2.0",
    "jest": "~23.6.0",
    "node-sass": "^4.11.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.6.0",
    "react-router-dom": "^4.3.1",
    "rimraf": "~2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "~23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-microsoft-contrib": "~5.2.1",
    "tsutils": "~3.5.0",
    "typedoc": "^0.13.0",
    "typescript": "~3.1.6",
    "url-loader": "^1.1.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "start": "node build/index.js",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "npm run lint && jest --coverage",
    "test:watch": "jest --watch",
    "docs": "typedoc src --out documentation --mode file --target es6 --theme default",
    "docs:json": "typedoc src --json docs/api/data.json --target es6",
    "docs:build": "webpack",
    "docs:serve": "webpack-dev-server",
    "preversion": "npm run clean && npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "author": {
    "name": "Josh Farrant",
    "email": "josh@farrant.me",
    "url": "https://farrant.me"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "bplist-creator": "0.0.7",
    "bplist-parser": "^0.1.1",
    "tslib": "~1.9.3",
    "uuid": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm test"
    }
  }
}
